

@model Data.Models.EduHistory





<div style="margin-left:10px">
    <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
        <li class="nav-item"><a href="#home" onclick="location.href='@Url.Action("Index", "PersonalDetail")?Id='+@Model.PersonalID" role="tab" data-toggle="tab">Personal</a></li>
      
        <li class="nav-item active"><a href="#educationHistory"  role="tab" data-toggle="tab">Education History</a></li>
        <li><a href="#history"  role="tab" onclick="location.href='@Url.Action("Index","JobHistory")?Id='+@Model.PersonalID" data-toggle="tab">JobHistory</a></li>
        <li class="dropdown" style="float:right">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-menu7"></i></a>
            <ul class="dropdown-menu dropdown-menu-right">

                @*<li onclick="addEduHistory()"><a><i class="icon-file-stats"></i> Add</a></li>*@
                <li><a data-EduId="@Model.EduHistoryID" onclick="EditEduHistory(this)"><i class="icon-file-stats"></i> Edit</a></li>
             
                <li><a><i class="icon-trash"></i> Delete</a></li>


            </ul>
        </li>
    </ul>

    <hr />
    <div class="tab-content" style="margin-left:10px;">

        <div class="tab-pane fade" id="home">
          
        </div>

        <div class="tab-pane fade in active" id="educationHistory">
            <div class="form-group">
                <label class="col-sm-3">SchoolOrCollegueOrUniversity</label>
                @Html.TextBoxFor(a => a.SchoolOrCollegueOrUniversity)
              
            </div>
            <div class="form-group">
                <label class="col-sm-3">Education</label>
                @Html.TextBoxFor(a => a.Education)
            </div>
            <div class="form-group">
                <label class="col-sm-3">FromDate</label>
                @Html.TextBoxFor(a => a.FromDate, "{0:dd/MM/yyyy}")
            </div>
            <div class="form-group">
                <label class="col-sm-3">ToDate</label>
                @Html.TextBoxFor(a => a.ToDate, "{0:dd/MM/yyyy}")
            </div>
            <div class="form-group">
                <label class="col-sm-3">SchoolAddress</label>
                @Html.TextBoxFor(a => a.SchoolAddress)
            </div>
            <div class="form-group">
                <label class="col-sm-3">Rank</label>
                @Html.TextBoxFor(a => a.Rank)
            </div>
            <div class="form-group">
                <label class="col-sm-3">LastSchool</label>
                @Html.TextBoxFor(a => a.LastSchool)
            </div>
            <div class="form-group">
                <label class="col-sm-3">StudentLifeActivity</label>
                @Html.TextBoxFor(a => a.StudentLifeActivity)
            </div>
        </div>

        <div class="tab-pane fade" id="history">
            <p>History Content : ...</p>
        </div>

      

    </div>



</div>





